journal = {Clinica Chimica Acta},
keywords = {COVID-19,Comirnaty vaccine,Neutralization,SARS-CoV-2,Serological standard,WHO},
month = {nov},
pages = {144--151},
pmid = {34425105},
publisher = {Elsevier},
title = {{Harmonization of six quantitative SARS-CoV-2 serological assays using sera of vaccinated subjects}},
