journal = {Irish Journal of Psychological Medicine},
keywords = {COVID-19,Comorbidity,Ireland,mental health,pandemic},
month = {dec},
number = {4},
pages = {272--277},
pmid = {33858542},
publisher = {Cambridge University Press},
title = {{Comorbidity and COVID-19: investigating the relationship between medical and psychological well-being}},
url = {https://www.cambridge.org/core/journals/irish-journal-of-psychological-medicine/article/comorbidity-and-covid19-investigating-the-relationship-between-medical-and-psychological-wellbeing/DB7C18BD64A9C7E67A3BEE11ADAE97D0},
