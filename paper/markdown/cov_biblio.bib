month = {jan},
number = {10},
pages = {3348--3359},
pmid = {33682276},
publisher = {Blackwell Publishing Ltd},
title = {{Neurological outcome and quality of life 3 months after COVID-19: A prospective observational cohort study}},
url = {https://covid19.elsevierpure.com/en/publications/neurological-outcome-and-quality-of-life-3-months-after-covid-19-},
